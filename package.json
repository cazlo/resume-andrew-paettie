{
  "name": "resume",
  "version": "4.0.0",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.4.2",
    "@material-ui/icons": "^2.0.0",
    "global": "^4.3.2",
    "lodash": "^4.17.11",
    "proxyquire": "^2.1.0",
    "react": "^16.5.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-transition-group": "^15.6.0",
    "react-devicon": "^0.1.9",
    "react-dom": "^16.5.2",
    "react-icons": "^3.1.0",
    "react-iframe": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.5",
    "react-scroll": "^1.7.10",
    "react-transition-group": "^2.4.0",
    "react-vertical-timeline-component": "^2.2.0",
    "rewire": "^4.0.1",
    "serve": "^10.0.1",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "prettier": "^1.14.3",
    "react-snap": "^1.19.0",
    "svg-inline-loader": "^0.8.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "fix-normalize-css-unit": "cross-env NODE_ENV=production babel --presets es2015 ./node_modules/normalize-css-unit/dist --out-dir ./node_modules/normalize-css-unit/dist",
    "start": "node scripts/customized-config  start",
    "build": "yarn fix-normalize-css-unit && node scripts/customized-config build",
    "build-snapshot": "react-snap",
    "build-run": "yarn build && serve -s build",
    "test": "node scripts/customized-config test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix --ignore-path .eslintignore src/"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
