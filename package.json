{
  "name": "resume",
  "version": "4.2.0",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.10",
    "devicon": "^2.12.0",
    "jest-junit": "^12.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "ngraph.graph": "0.0.14",
    "ngraph.path": "^1.0.2",
    "prop-types": "^15.7.2",
    "proxyquire": "^2.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-scroll": "^1.8.3",
    "react-snap": "^1.23.0",
    "react-vertical-timeline-component": "^2.2.0",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "rewire": "^5.0.0",
    "serve": "^12.0.0",
    "styled-components": "^5.3.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "(react-scripts build && react-snap || true) && npm run replace-noscript",
    "build-run": "npm run build && serve -s build",
    "test": "react-scripts test ",
    "int-test": "jest -c integration-test.config.js --watch",
    "test-ci": "jest -c jest.config.js",
    "int-test-ci": "jest -c integration-test.config.js",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "lint-watch": "jest -c lint.config.js --watch",
    "replace-noscript": "node src/buildutil/noscript.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest-junit": {
    "ouputDirectory": "./coverage"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true
    }
  },
  "reactSnap": {
    "source": "build",
    "destination": "build/noscript",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": true
    }
  }
}
