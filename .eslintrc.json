{
  "parser": "babel-eslint",
  "extends": [
//    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "prettier",
    "react",
    "jsx-a11y"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "max-len": ["error", {"code": 120}],
    "react/prop-types": ["warn"],
    "prettier/prettier": "error",
    "no-restricted-syntax": "warn",
    "react/jsx-filename-extension": "warn"
  }
}
